lista # https://cdn.jsdelivr.net/gh/angelonuffer/lista@2efb5d2699ae928fc7d2b29be6783ae06670415a/código/0

{ comandos } => lista.mapear({
  lista: comandos
  função: comando =>
    | comando.tipo == "escreva" = [
      "console.log('"
      comando.texto
      "')"
    ] * ""
    | comando.tipo == "saia" = [
      "process.exit("
      "" + comando.código
      ")"
    ] * ""
    | "throw new Error('Comando desconhecido.')"
}) * ";"